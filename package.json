{
  "name": "booking-service",
  "version": "2.0.0",
  "description": "This module powers the booking interface of a vacation rental website.",
  "author": "Cinzia Borello",
  "license": "ISC",
  "engines": {
    "node": ">=6.13.0"
  },
  "scripts": {
    "seed": "node database/seedData.js",
    "load": "node postgres/postgresLoader.js",
    "start": "node server/index.js",
    "build": "webpack --mode production",
    "start:dev": "nodemon server/index.js",
    "build:dev": "webpack -d --watch",
    "test": "jest"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bootstrap": "^3.3.7",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "mysql": "^2.18.1",
    "path": "^0.12.7",
    "pg": "^8.3.3",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-axios": "^2.0.3",
    "react-bootstrap": "^1.3.0",
    "react-dom": "^16.13.1",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "chai": "^4.2.0",
    "css-loader": "^4.2.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.3",
    "enzyme-to-json": "^3.5.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.0",
    "extract-loader": "^5.1.0",
    "file-loader": "^6.0.0",
    "html-loader": "^1.2.1",
    "jest": "^26.3.0",
    "nodemon": "^2.0.4",
    "sinon": "^9.0.3",
    "style-loader": "^1.2.1",
    "ttf-loader": "^1.0.2",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "jest": {
    "verbose": true,
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "client"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "<rootDir>/client/__tests__/*.js",
      "!<rootDir>/client/index.jsx"
    ],
    "coverageReporters": [
      "text"
    ],
    "testRegex": "((\\.|/*.)(spec))\\.js?$",
    "setupFilesAfterEnv": [
      "<rootDir>/client/__tests__/jest.config.js"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/client/__tests__/assetsTransformer.js",
      "\\.(css|less)$": "<rootDir>/client/__tests__/assetsTransformer.js"
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "main": "server/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SDC-7/booking-service.git"
  },
  "bugs": {
    "url": "https://github.com/SDC-7/booking-service/issues"
  },
  "homepage": "https://github.com/SDC-7/booking-service#readme"
}
